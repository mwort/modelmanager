#/usr/bin/env python

import modelmanager.project
from modelmanager.settings import Function
from modelmanager.commandline import execute_from_commandline


try:
    project = modelmanager.project.Project()
    functions = project.settings.functions
    plugins = project.settings.plugins
except modelmanager.project.ProjectDoesNotExist:
    functions = {'setup': Function(modelmanager.project.setup)}
    plugins = {}

print(execute_from_commandline(functions, plugins))
